fmt := import("fmt")
exch := import("exchange")

name := "run"
timer := "5s"

load := func() {
    tx := exch.ticker("BTC Markets", "BTC-AUD", "-", "SPOT")
    fmt.printf("TICKER DATA: %v\n", tx)
    cb := exch.orderbook("BTC Markets", "BTC-AUD", "-", "SPOT")
    fmt.printf("OB DATA: %v\n", cb)
}

load()